
config nodogsplash
	option redirecturl 'http://www.qq.com'
	option webroot '/etc/nodogsplash/htdocs'
	option gatewayinterface 'br-lan'
	option gatewayname 'AD SYSTEM'
	option maxclients '250'
	option clientidletimeout '120'
	option clientforcetimeout '120'
	list authenticated_users 'allow all'
	list preauthenticated_users 'allow tcp port 80 to 192.168.0.0/16'
	list preauthenticated_users 'allow tcp port 80 to jd.com'
	list preauthenticated_users 'allow tcp port 80 to kkk.com'
	list users_to_router 'allow tcp port 22'
	list users_to_router 'allow tcp port 23'
	list users_to_router 'allow tcp port 53'
	list users_to_router 'allow udp port 53'
	list users_to_router 'allow udp port 67'
	list users_to_router 'allow tcp port 80'
	list fw_mark_authenticated '520'
	list fw_mark_trusted '510'
	list fw_mark_blocked '500'
	option enabled '0'
	list allow_site 'jd.com'
	list allow_site 'kkk.com'

